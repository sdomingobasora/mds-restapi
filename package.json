{
  "name": "restapi",
  "version": "1.0.0",
  "description": "RestAPI server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "yarn build && cross-env NODE_ENV=production node dist/server.js",
    "build": "swc src -d dist --source-maps --copy-files",
    "test:unit": "jest --forceExit --detectOpenHandles ./src/",
    "test:e2e": "jest --forceExit --detectOpenHandles ./e2e/",
    "lint": "eslint --ext ts src/",
    "deploy:prod": "yarn start",
    "release": "semantic-release"
  },
  "dependencies": {
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.220",
    "@swc/helpers": "0.4.14",
    "dotenv": "16.0.1",
    "express": "4.18.1"
  },
  "devDependencies": {
    "@codedependant/semantic-release-docker": "4.3.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.6",
    "@types/node": "17.0.45",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.1",
    "node-config": "0.0.2",
    "node-gyp": "9.1.0",
    "prettier": "2.8.4",
    "semantic-release": "20.1.0",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "volta": {
    "node": "18.14.0"
  }
}
